{
  "name": "kvgrav",
  "packageManager": "yarn@3.2.2",
  "version": "0.0.1",
  "description": "KVG Gravitation",
  "main": "lib/index.js",
  "browser": {
    "./lib/index.js": "./lib/browser.js"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "dev": "tsc --watch",
    "prettier": "prettier --write src/* test/*",
    "tslint": "tslint -p tslint -p tsconfig.json",
    "codestyle": "npm run prettier && npm run tslint",
    "docsify": "docsify serve ./docs",
    "typedoc": "typedoc --out ./docs/api ./src --readme docs/README.md",
    "docs": "npm run typedoc && npm run docsify"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run codestyle"
    }
  },
  "keywords": [
    "gravitation",
    "katyushik"
  ],
  "author": "webcounters (https://github.com/counters)",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^12.7.5",
    "docsify-cli": "^4.3.0",
    "husky": "^3.0.5",
    "jest": "^28.1.3",
    "prettier": "^1.18.2",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.23.9",
    "typescript": "^4.7.4"
  }
}
